{% set entries = craft.haxor.getAllArticles() %}
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/1.0">
    {% for entry in entries %}
        <url>
            <loc>{{ entry.url }}</loc>
            <lastmod>{{ entry.dateUpdated|date("c") }}</lastmod>
        </url>
    {% endfor %}
</urlset>